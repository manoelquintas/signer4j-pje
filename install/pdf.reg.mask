[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\Shell\PjeOffice\shell]


[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\Shell\PjeOffice\shell\Z002ZEW]
"MUIVerb"="Assinar e salvar na mesma pasta"
"Icon"="%PJEOFFICE_HOME%\\explorer\\sign.ico"
[HKEY_CLASSES_ROOT\SystemFileAssociations\.pdf\Shell\PjeOffice\shell\Z002ZEW\command]
